load pipes.cat as P
alias java.lang.Iterable.iterator as iterator
alias java.util.Iterator.next as value

<Root> {
  iter = iterator(list)
  repeat {
     value = value(iter)
     publish "element = 'value'\n"
  }
  <Gluon>
  publish "'java.lang.Integer.parseInt('123')'\n"
  publish "'java.lang.Integer.toString(123)'\n"
  <P:OutA> >> <P:InAThenOutB> >> <P:InB>
  <P:OutA> >> aout
  publish "'aout'\n"
  return <P:InAThenOutB> << "a"
}
<Gluon> {
  test = (org.fuwjin.sample.SingleField){
    value:123
  }
  publish "check it: 'org.fuwjin.sample.SingleField.value(test)'\n"
}
