<RenderFunction>{
  either <Constructor> >> render
  or <FieldAccess> >> render
  or <FieldMutator> >> render
  or <InstanceOf> >> render
  or <Method> >> render
  or <StaticFieldAccess> >> render
  or <StaticFieldMutator> >> render
  or <StaticMethod> >> render
  or render = <Composite>
  assume not hasNext(params)
  return render
