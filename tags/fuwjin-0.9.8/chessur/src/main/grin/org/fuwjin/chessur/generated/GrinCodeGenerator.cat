  publish ";"
  setters = iterator(ObjectTemplate.setters(value))
  could repeat{
    field = getNext(setters)
    function = FieldTemplate.setter(field)
    value = FieldTemplate.value(field)
    either <MemberFieldMutator>
    or <MemberMethod>
    or <Composite>
